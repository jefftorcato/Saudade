<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jefftorcato.saudadeartist" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SaudadeArtist</name>
    <description>
        A partner app for artists to add events
    </description>
    <author email="jefftorcato@gmail.com" href="https://jefftorcato.in">
        Jeff Torcato
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="loadUrlTimeoutValue" value="700000" />
    <preference name="android-minSdkVersion" value="23" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
